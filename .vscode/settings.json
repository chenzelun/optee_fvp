{
    "files.autoSave": "afterDelay",
    "files.exclude": {
        "**/.a": true,
        "**/.app": true,
        "**/.cmd": true,
        "**/.d": true,
        "**/.dll": true,
        "**/.DS_Store": true,
        "**/.dSYM/**": true,
