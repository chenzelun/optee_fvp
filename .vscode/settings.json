        "**/.hex": true,
        "**/.hg": true,
        "**/.i*86": true,
        "**/.idb": true,
        "**/.ilk": true,
        "**/.ko": true,
        "**/.l": true,
        "**/.la": true,
        "**/.lib": true,
        "**/.map": true,
        "**/.mod*": true,
        "**/.o": true,
        "**/.obj": true,
        "**/.out": true,
        "**/.pch": true,
        "**/.pdb": true,
        "**/.repo/**": true,
        "**/.so": true,
        "**/.so.*": true,
        "**/.su": true,
        "**/.svn": true,
